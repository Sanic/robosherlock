<launch>
   <arg name="ip_address"              default="192.168.102.3"/>
   <arg name="port"                    default="9000"/>
   <param name="ip_address"              type="str" value="$(arg ip_address)"/>
   <param name="port"                    type="str" value="$(arg port)"/>
   <node name="robotvqa_launcher" pkg="robosherlock" type="robotvqa_launcher.py" output="screen"/>
</launch>
